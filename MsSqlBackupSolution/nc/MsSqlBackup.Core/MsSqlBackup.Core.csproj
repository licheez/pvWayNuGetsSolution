<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <AssemblyName>MsSqlBackup.Core</AssemblyName>
    <RootNamespace>pvWay.MsSqlBackup.Core</RootNamespace>
    <Authors>Pierre Van Wallendael</Authors>
    <Company>pvWay Ltd</Company>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Copyright>pvWay Ltd 2020</Copyright>
    <PackageId>pvWay.MsSqlBackup.Core</PackageId>
    <Description>Backup any Ms Sql database to a local or network folder and execute maintenance plans</Description>
    <PackageReleaseNotes>fix vulnerability</PackageReleaseNotes>
    <PackageIcon>pvwayLogoTextLess256.png</PackageIcon>
    <PackageProjectUrl>https://github.com/licheez/pvWayNuGetsSolution/tree/master/MsSqlBackupSolution/nc/MsSqlBackup.Core</PackageProjectUrl>
    <PackageTags>pvWay Ms Sql Backup</PackageTags>
    <PackageReadmeFile>readMe.md</PackageReadmeFile>
    <Version>1.0.5</Version>
    <RepositoryUrl>https://github.com/licheez/pvWayNuGetsSolution/tree/master/MsSqlBackupSolution/nc/MsSqlBackup.Core</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="System.Data.SqlClient" Version="4.8.6" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\..\..\pvwayLogoTextLess256.png">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
    <None Update="readMe.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
  </ItemGroup>

</Project>
