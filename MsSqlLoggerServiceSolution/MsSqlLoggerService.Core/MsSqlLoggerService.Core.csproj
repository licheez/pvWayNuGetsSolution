<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <RootNamespace>pvWay.MsSqlLoggerService.Core</RootNamespace>
    <PackageId>pvWay.MsSqlLoggerService.Core</PackageId>
    <Authors>Pierre Van Wallendael</Authors>
    <Company>pvWay</Company>
    <PackageIcon>Logo150x200px.png</PackageIcon>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Description>Ms Sql implementation of MethodResultWrapper ILoggerService. Persists logs into a MsSql table.</Description>
    <Copyright>Copyright pvWay 2020</Copyright>
    <PackageProjectUrl>https://github.com/licheez/pvWayNuGetsSolution/tree/master/MsSqlLoggerServiceSolution/MsSqlLoggerService.Core</PackageProjectUrl>
    <RepositoryUrl>https://github.com/licheez/pvWayNuGetsSolution/tree/master/MsSqlLoggerServiceSolution/MsSqlLoggerService.Core</RepositoryUrl>
    <RepositoryType>GitHub</RepositoryType>
    <PackageTags>pvWay MsSql Logger MethodResultWrapper</PackageTags>
    <PackageReleaseNotes>Initial version for Core</PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <None Include="Logo150x200px.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <None Include="MsSqlLoggerServiceReadMe.md">
      <Pack>true</Pack>
      <PackagePath>/</PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="pvWay.MethodResultWrapper.Core" Version="1.0.0" />
    <PackageReference Include="System.Data.SqlClient" Version="4.8.1" />
  </ItemGroup>

</Project>
