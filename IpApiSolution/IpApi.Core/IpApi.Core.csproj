<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <RootNamespace>pvWay.IpApi.Core</RootNamespace>
    <AssemblyName>pvWay.IpApi.Core</AssemblyName>
    <ApplicationIcon>pvWayLogoTextLess.ico</ApplicationIcon>
    <Authors>Pierre Van Wallendael</Authors>
    <Company>pvWay</Company>
    <Description>Geolocalize an Ip address. The service uses the free plan from the https://ipapi.co/ service</Description>
    <Copyright>pvWay 2020</Copyright>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageIcon>pvwayLogoTextLess256.png</PackageIcon>
    <PackageTags>Free ip geo localization</PackageTags>
    <PackageReleaseNotes>initial version</PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\..\pvwayLogoTextLess256.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
