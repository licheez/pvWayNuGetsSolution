<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <RootNamespace>pvWay.IpApi.Core</RootNamespace>
    <AssemblyName>pvWay.IpApi.Core</AssemblyName>
    <ApplicationIcon>pvWayLogoTextLess.ico</ApplicationIcon>
    <Authors>Pierre Van Wallendael</Authors>
    <Company>pvWay</Company>
    <Description>Provides geolocalization info for a given ip. This service is based on IpStack API.</Description>
    <Copyright>pvWay 2020</Copyright>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageIcon>pvwayLogoTextLess256.png</PackageIcon>
    <PackageTags>Free ip geo localization IpStack</PackageTags>
    <PackageReleaseNotes>breaking change: all primitives made nullable</PackageReleaseNotes>
    <PackageProjectUrl>https://github.com/licheez/pvWayNuGetsSolution/tree/master/IpApiSolution/IpApi.Core</PackageProjectUrl>
    <RepositoryUrl>https://github.com/licheez/pvWayNuGetsSolution/tree/master/IpApiSolution/IpApi.Core</RepositoryUrl>
    <RepositoryType>GitHub</RepositoryType>
    <Version>3.0.1</Version>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\..\pvwayLogoTextLess256.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
  </ItemGroup>

</Project>
