<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <RootNamespace>pvWay.MsSqlMultiPartVarChar.nc6</RootNamespace>
    <ApplicationIcon>pvWayLogoTextLess.ico</ApplicationIcon>
    <PackageId>pvWay.MsSqlMultiPartVarChar.nc6</PackageId>
    <Version>1.0.0</Version>
    <Authors>Pierre Van Wallendael</Authors>
    <Company>pvWay</Company>
    <Product>pvWay.MsSqlMultiPartVarChar.nc6</Product>
    <Description>Persists multi part text values (dictionary string - string) into one single VARCHAR column into an Ms SQL Db (2014 or &gt;=). The package contains a cSharp class for storing/retreiving the dictionary to/from the field and the SQL code for creating a scalar function for stored procedure implementation.</Description>
    <Copyright>pvWay ltd 2023</Copyright>
    <PackageProjectUrl>https://github.com/licheez/pvWayNuGetsSolution/tree/master/MsSqlMultiLangVarChar/Core</PackageProjectUrl>
    <PackageIcon>pvwayLogoTextLess256.png</PackageIcon>
    <PackageReadmeFile>ReadMe.md</PackageReadmeFile>
    <RepositoryUrl>https://github.com/licheez/pvWayNuGetsSolution/tree/master/MsSqlMultiLangVarChar/Core</RepositoryUrl>
    <RepositoryType>GitHub</RepositoryType>
    <PackageTags>iso639 translations resources multi lingual pvWay MsSql</PackageTags>
    <PackageReleaseNotes>initial nc6 version</PackageReleaseNotes>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
  </PropertyGroup>

  <ItemGroup>
    <Content Include="pvWayLogoTextLess.ico" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\..\..\pvwayLogoTextLess256.png">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <None Update="ReadMe.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

</Project>
