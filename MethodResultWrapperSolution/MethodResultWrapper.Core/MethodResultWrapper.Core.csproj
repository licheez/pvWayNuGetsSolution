<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <RootNamespace>pvWay.MethodResultWrapper.Core</RootNamespace>
    <PackageId>pvWay.MethodResultWrapper.Core</PackageId>
    <Authors>Pierre Van Wallendael</Authors>
    <Company>pvWay</Company>
    <Description>Provides a generic wrapper that returns whether or not a method succeeded or failed carrying the method result on success or a list of notifications/errors in case of failure</Description>
    <Copyright>Copyright pvWay 2020</Copyright>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/licheez/pvWayNuGetsSolution/tree/master/MethodResultWrapperSolution/MethodResultWrapper.Core</PackageProjectUrl>
    <iconUrl>http://www.pvway.com/images/Logo150x200px.png</iconUrl>

    <PackageTags>pvway methodresult logger</PackageTags>
    <PackageReleaseNotes>DsoHttpResult.HttpStatusCode made public</PackageReleaseNotes>
    <PackageIcon>Logo150x200px.png</PackageIcon>
    <RepositoryUrl>https://github.com/licheez/pvWayNuGetsSolution/tree/master/MethodResultWrapperSolution/MethodResultWrapper.Core</RepositoryUrl>
    <RepositoryType>GitHub</RepositoryType>
    <Version>1.0.1</Version>
  </PropertyGroup>

  <ItemGroup>
    <Content Include="MethodResultWrapperReadMe.md">
      <Pack>true</Pack>
      <PackagePath>/</PackagePath>
    </Content>
  </ItemGroup>

  <ItemGroup>
    <None Include="Logo150x200px.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
