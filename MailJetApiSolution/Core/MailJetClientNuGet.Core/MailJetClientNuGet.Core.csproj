<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <AssemblyName>pvWay.MailJetClientNuGet.Core</AssemblyName>
    <RootNamespace>pvWay.MailJetClientNuGet.Core</RootNamespace>
    <Authors>Pierre Van Wallendael</Authors>
    <Company>pvWay</Company>
    <Description>Simple interface for posting emails via MailJet templates</Description>
    <Copyright>Copyright pvWay 2023</Copyright>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/licheez/pvWayNuGetsSolution/tree/master/MailJetApiSolution/Fw/MailJetClientNuGet.Core</PackageProjectUrl>
    <PackageIcon>pvwayLogoTextLess256.png</PackageIcon>
    <RepositoryUrl>https://github.com/licheez/pvWayNuGetsSolution/tree/master/MailJetApiSolution/Fw/MailJetClientNuGet.Core</RepositoryUrl>
    <RepositoryType>Github</RepositoryType>
    <PackageTags>MailJet Template Sender API 3.1 Post Email</PackageTags>
    <PackageReleaseNotes>upgrade Newtonsoft.Json version</PackageReleaseNotes>
    <Version>1.0.3</Version>
    <PackageReadmeFile>ReadMe.md</PackageReadmeFile>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\..\..\pvwayLogoTextLess256.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
  </ItemGroup>

  <ItemGroup>
    <None Update="ReadMe.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

</Project>
